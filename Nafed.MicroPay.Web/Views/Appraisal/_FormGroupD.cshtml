                        @Html.TextBoxFor(model => model.formGroupDHdr.WorkedPeriodUnderROTo, String.Format("{0:dd/MM/yyyy}", Model.formGroupDHdr.WorkedPeriodUnderROTo), new { @type = "text", @class = "form-control datepicker", @placeholder = "dd/mm/yyyy" })
