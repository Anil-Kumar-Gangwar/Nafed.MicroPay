@using Nafed.MicroPay.Model
@model EmpAchievementAndCertificationDocument
<tr class="filesvalidate">
   

    <td>
        @Model.Sno
    </td>
    <td>
        @Html.EditorFor(model => model.DocumentTitle, new { htmlAttributes = new { @class = "form-control name", @maxlength = "100", @autocomplete = "off" } })

    </td>

    <td>
        <input type="file" name="Files" class="file" id="Files" accept=".jpg,.jpeg,.png,.pdf,.doc,.docx" />

    </td>
    <td class="text-center">
        <input type="checkbox" name="record">
    </td>

</tr>