@using Nafed.MicroPay.Model
@model List<UnAssignedPFRecord>

@{ 

    Layout = null;
}


<div class="" style="margin-top:10px;">

    <table id="tblGrid" class="table table-centered table-striped dt-responsive w-100 mt-2">
        <thead>
            <tr>
                <th>
                    #
                </th>
                <th>
                    EmpCode
                </th>
                <th>
                    Name
                </th>
                <th>
                    Branch
                </th>
                <th>
                    Department
                </th>
                <th>
                    Designation
                </th>

                @*<th>
                    PF No
                </th>*@

                <th>
                    Action
                </th>
            </tr>
        </thead>
        <tbody>
            @{
                if (Model.Count() > 0)
                {
                    var sno = 1;
                    for (int i = 0; i < Model.Count(); i++)
                    {
                        {
                            Model[i].SNo = sno;
                        }
                        <tr id="@sno">
                   
                                @Html.Partial("_ListItemRow", Model[i])
                          
                        </tr>

                        sno++;
                    }
                }
            }

        </tbody>
    </table>


</div>