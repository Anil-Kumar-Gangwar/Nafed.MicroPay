@model MicroPay.Web.Models.APARSkillSetViewModel

<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 mb-12">
        <div class="form-group">
            <div class="row">
                <div class="col-lg-2 col-md-2 col-sm-12">
                    Department :
                </div>
                <div class="col-lg-3 col-md-3 col-sm-12 h5 pl-lg-0">
                    @Model.APARSkillSet.Department
                </div>
                <div class="col-lg-2 col-md-2 col-sm-12">
                    Designation :
                </div>
                <div class="col-lg-3 col-md-3 col-sm-12 h5 pl-lg-0">
                    @Model.APARSkillSet.Designation
                </div>
            </div>
        </div>
        </div>
    </div>
<table id="tblGrid" class="table table-centered table-striped dt-responsive w-100">
    <thead>
        <tr>
            <th>
                #
            </th>
            <th>
                Skill Type
            </th>

            <th>
                Skills
            </th>

        </tr>
    </thead>
    <tbody>
        @{
            if (Model.APARSkillSetDtlList.Count() > 0)
            {
                var sno = 1;
                foreach (var item in Model.APARSkillSetDtlList)
                {
                    <tr>
                        <td> @sno</td>
                        <td> @item.SkillType</td>
                        <td> @item.Skill</td>
                    </tr>
                    sno++;
                }
            }
        }

    </tbody>
</table>


