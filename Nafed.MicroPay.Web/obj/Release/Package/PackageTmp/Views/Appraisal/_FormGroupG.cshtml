<script type="text/javascript">

    $('.noselect').find('input, textarea, button, select,radio').attr('readonly', 'readonly');

    $('.noselect').find(':radio:not(:checked)').attr('disabled', true);

    $("#formGroupGHdr_WorkedPeriodUnderROFrom").datepicker({ changeYear: true, dateFormat: 'dd/mm/yy' });
    $("#formGroupGHdr_WorkedPeriodUnderROTo").datepicker({ changeYear: true, dateFormat: 'dd/mm/yy' });

    function OnSuccess(data) {
        debugger;
        if (data.isTraining == 1) {
            $("#div-main-form").html(data.htmlData);
        }
        else if (data.msgType == undefined) {
            $("#div-main-form").html(data);
        }

        if ((data.formState != undefined && data.formState != 0) && (parseInt(data.formState) % 2) == 0)
            $(".div-action").addClass('hide');
        if (data.part == 0)
            $("#div-main-form").html(data.htmlData);

        if (data.msgType == "success") {
            commonMessage(data.msgType, data.msg);
            //if (data.formState == 1) {
            window.location.reload();
            @*}
            else if (data.formState == 2) {
                window.location.href = "@Url.Action("Index")";
            }*@
        }


        if (data.msgType == "error") {
            commonMessage(data.msgType, data.msg);
        }
    }

    calculateChkChecked();

    $(".class-A,.class-B,.class-C,.class-D,.class-E").change(function () {
        calculateChkChecked();
    });

    function calculateChkChecked() {

        var A_checkedCount = 0; var B_checkedCount = 0; var C_checkedCount = 0; var D_checkedCount = 0; var E_checkedCount = 0;

        $('.class-A').each(function (idx) {
            A_checkedCount += $(this).is(":checked") ? 1 : 0;
        });
        $('.class-B').each(function (idx) {
            B_checkedCount += $(this).is(":checked") ? 1 : 0;
        });
        $('.class-C').each(function (idx) {
            C_checkedCount += $(this).is(":checked") ? 1 : 0;
        });
        $('.class-D').each(function (idx) {
            D_checkedCount += $(this).is(":checked") ? 1 : 0;
        });
        $('.class-E').each(function (idx) {
            E_checkedCount += $(this).is(":checked") ? 1 : 0;
        });

        var total = "A=" + A_checkedCount + "; B=" + B_checkedCount + "; C=" + C_checkedCount + "; D=" + D_checkedCount + "; E=" + E_checkedCount + "";
        $("#total_rating").html(total);
    }

    function radioOnChange(controlID) {
        var jj = $("#" + controlID).val();
        if (controlID == "Integrity_1" || controlID == "Integrity_2" || controlID == "Integrity_3") {
            $("#formGroupGHdr_INTEGRITY").val($("#" + controlID).val());
        }
    }

</script>

