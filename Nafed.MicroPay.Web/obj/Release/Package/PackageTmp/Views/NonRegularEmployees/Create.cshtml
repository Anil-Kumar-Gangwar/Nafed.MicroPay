
@{
    ViewBag.Title = "Non Regular Employees";
    Layout = "~/Views/Shared/_Layout.cshtml"; 
}

@*<script type="text/javascript" src="@Url.Content("~/Scripts/jquery.unobtrusive-ajax.min.js")"></script>*@
<div class="mt-2">
    <div class="tab" role="tabpanel">

        <nav class="navbar navbar-expand-md  navbar-dark px-0">

            <!-- Brand -->
         

            <!-- Toggler/collapsibe Button -->
            <button class="navbar-toggler bg-info" type="button" data-toggle="collapse" data-target="#collapsible_tab">
                <span class="fa fa-bars"></span>
            </button>

            <!-- Navbar links -->
            <div class="collapse navbar-collapse" id="collapsible_tab" style="position: relative;z-index: 999;">
                <ul class="nav nav-tabs" role="tablist" id="tabs">
                    <li class="nav-item">
                        <a class="nav-link active toggle-tooltip" title="General" role="tab" data-toggle="tab" href="#general">General</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link toggle-tooltip " role="tab" data-toggle="tooltip" title="Personal" href="#Personal">Personal</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link toggle-tooltip " role="tab" data-toggle="tooltip" title="Contract Extension" href="#extention">Contract Extension</a>
                    </li>
                </ul>
            </div>
        </nav>
        <!-- Tab panes -->
        <div class="tab-content">
            <div id="general" class="container tab-pane active fullwidth-container">
                @Html.Action("_CreateGeneralDetails", "NonRegularEmployees")
            </div>
            <div id="Personal" class="container tab-pane fade fullwidth-container">
                @*@Html.Action("_PersonalDetails", "NonRegularEmployees")*@
            </div>
            <div id="extention" class="container tab-pane fade fullwidth-container">
                @*@Html.Action("_ContractExtensionList", "NonRegularEmployees")*@
            </div>
        </div>
    </div>
    </div>


