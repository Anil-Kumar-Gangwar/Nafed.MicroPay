
@{
    ViewBag.Title = "Ticket";
    ViewBag.NavigationTitle = "Assign Ticket";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div id="div-view-ticket">
   
    <div class="card_new_header">
        @using (Ajax.BeginForm("GetTicketList", "Ticket", new AjaxOptions { HttpMethod = "Get", UpdateTargetId = "div-TicketList" }))
        {
        @Html.Partial("_FilterSectionHD")
        }
    </div>
   
    <div class="card_new_body">
        <div id="div-TicketList">
            @Html.Action("TicketList")
        </div>
    </div>

</div>

<div class="modal" id="div-remarks-popup">
    <div class="modal-dialog modal-md">
        <div class="modal-content">

            <!-- Modal Header -->
            <div class="modal-header">
                <h4 class="modal-title">Message</h4>
                <button type="button" class="close" data-dismiss="modal">&times;</button>
            </div>

            <!-- Modal body -->
            <div class="modal-body" id="div-remarks">

            </div>

            <!-- Modal footer -->
            <div class="modal-footer">

            </div>

        </div>
    </div>
</div>








